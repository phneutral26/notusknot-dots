super + Return
    urxvt
super + space
    dmenu_run
super + d 
    firefox
super + p
    nautilus
super + m
    sudo shutdown now

super + alt + {q,r}
    bspc {quit,wm -r}
super + {_,shift + } + q
    bspc node -{c,k}
super + {_,shift + }{1-5}
    bspc {desktop -f, node -d} '^{1-5}
super + {t,s,f}
    bspc node -t {tiled,floating,fullscreen}
